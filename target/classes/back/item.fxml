<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="itemC" alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="1123.0" spacing="5" style="-fx-background-color: #1f1f17; -fx-background-radius: 5; -fx-background-insets: 0;" stylesheets="@../styles/styleitem.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.back.UserItemController">
    <children>
      <Label fx:id="firstname" prefHeight="17.0" prefWidth="67.0" text="firstname" textFill="#FFFFFF">
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin></Label>
      <Label fx:id="lastname" prefHeight="17.0" prefWidth="63.0" text="lastname" textFill="#FFFFFF">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>
      <Label fx:id="email" prefHeight="17.0" prefWidth="221.0" text="email" textFill="#FFFFFF">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>
      <Label fx:id="role" prefHeight="17.0" prefWidth="58.0" text="role" textFill="#FFFFFF">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>

      <Label fx:id="status" prefHeight="17.0" prefWidth="61.0" text="status" textFill="#FFFFFF" />
      <Label fx:id="lastlogin" prefHeight="17.0" prefWidth="153.0" text="last login" textFill="#FFFFFF" />
      <Label fx:id="datecreated" prefHeight="17.0" prefWidth="152.0" text="date created" textFill="#ffffff" />
      <Button fx:id="deletebutton" mnemonicParsing="false" prefHeight="27.0" prefWidth="78.0" style="-fx-border-color: #ffd721; -fx-border-radius: 20; -fx-background-color: transparent;" stylesheets="@../styles/styleitem.css" text="delete">
         <HBox.margin>
             <Insets bottom="5" left="5" right="5" top="5" />
         </HBox.margin>
      </Button>
      <Button fx:id="updatebutton" mnemonicParsing="false" prefHeight="27.0" prefWidth="78.0" style="-fx-border-color: #ffd721; -fx-border-radius: 20; -fx-background-color: transparent;" stylesheets="@../styles/styleitem.css" text="update">
         <HBox.margin>
            <Insets />
         </HBox.margin></Button>
      <Button fx:id="feedbackButton" mnemonicParsing="false" prefHeight="27.0" prefWidth="78.0" style="-fx-border-color: #ffd721; -fx-border-radius: 20; -fx-background-color: transparent;" stylesheets="@../styles/styleitem.css" text="Feecback">
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin>
      </Button>
   </children>
    <effect>
        <DropShadow blurType="GAUSSIAN" color="#444444" radius="5.0" spread="0.2" />
    </effect>
</HBox>
